{
  "compilerOptions": {
    "module": "commonjs",
    "outDir": "dist/",
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "target": "es5"
  },
  "include": [
    "app/scripts/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "**/*.spec.ts",
    "typings/*"
  ]
}